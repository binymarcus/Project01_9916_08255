<Window x:Class="PLWPF.AdminFolder.Host.numOfHUforEachHostForAdmin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF.AdminFolder.Host"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="numOfHUforEachHostForAdmin" Height="450" Width="800">
    <Grid>
        <Button x:Name="showB" Content="Show" HorizontalAlignment="Left" Margin="10,64,0,0" VerticalAlignment="Top" Width="318" Height="40" Click="showB_Click"/>
        <xctk:WatermarkTextBox x:Name="head" HorizontalAlignment="Left" Height="49" Margin="10,10,0,0" TextWrapping="Wrap" FontSize="20" Watermark="enter num of hosting units to find:" VerticalAlignment="Top" Width="318"/>
        <Button x:Name="backB" Content="Back" HorizontalAlignment="Left" Margin="10,379,0,0" VerticalAlignment="Top" Width="318" Height="32" Click="backB_Click"/>
        <ScrollViewer x:Name="scrollview1" HorizontalAlignment="Left" Height="401" Margin="333,10,0,0" VerticalAlignment="Top" Width="450">
            <StackPanel
                x:Name="b" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="auto" Width="auto">
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Window>
